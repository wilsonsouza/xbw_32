<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup Condition="'$(Force-Disable-cpprestsdk-v120-winxp-msvcstl-dyn-rt-dyn)' != '' Or '$(PlatformToolset)' != 'v120_xp' Or '$(ApplicationType)' != ''">
