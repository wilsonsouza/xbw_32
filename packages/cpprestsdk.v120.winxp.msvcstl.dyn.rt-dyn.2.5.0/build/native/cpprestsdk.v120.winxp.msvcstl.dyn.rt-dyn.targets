  </PropertyGroup>
  <ItemDefinitionGroup Condition="'$(Force-Disable-cpprestsdk-v120-winxp-msvcstl-dyn-rt-dyn)' == ''">
    <ClCompile>
      <AdditionalOptions Condition="'$(PlatformToolset)'=='v140'">/d2notypeopt %(AdditionalOptions)</AdditionalOptions>
      <PreprocessorDefinitions Condition="'$(PlatformToolset)' == 'v120_xp'">CPPREST_TARGET_XP;%(PreprocessorDefinitions)</PreprocessorDefinitions>
    </ClCompile>
    <Link>
      <LibraryDependencies Condition="'$(PlatformToolset)' == 'Clang_3_4'">m;%(LibraryDependencies)</LibraryDependencies>
    </Link>
  </ItemDefinitionGroup>
  <ItemDefinitionGroup Condition="'$(Force-Disable-cpprestsdk-v120-winxp-msvcstl-dyn-rt-dyn)' == ''">
    <Link>
      <AdditionalDependencies Condition="'$(Configuration)' == 'Debug' And '$(Platform)' == 'Win32'">$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\Win32\Debug\cpprest120d_xp_2_5.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalDependencies Condition="'$(Configuration)' == 'Release' And '$(Platform)' == 'Win32'">$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\Win32\Release\cpprest120_xp_2_5.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalDependencies Condition="'$(Configuration)' == 'Debug' And '$(Platform)' == 'x64'">$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\x64\Debug\cpprest120d_xp_2_5.lib;%(AdditionalDependencies)</AdditionalDependencies>
      <AdditionalDependencies Condition="'$(Configuration)' == 'Release' And '$(Platform)' == 'x64'">$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\x64\Release\cpprest120_xp_2_5.lib;%(AdditionalDependencies)</AdditionalDependencies>
    </Link>
    <ClCompile>
      <AdditionalIncludeDirectories>$(MSBuildThisFileDirectory)include;%(AdditionalIncludeDirectories)</AdditionalIncludeDirectories>
    </ClCompile>
  </ItemDefinitionGroup>
  <ItemGroup Condition="'$(Configuration)' == 'Debug' And '$(Platform)' == 'Win32' And '$(Force-Disable-cpprestsdk-v120-winxp-msvcstl-dyn-rt-dyn)' == ''">
    <ReferenceCopyLocalPaths Include="$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\Win32\Debug\cpprest120d_xp_2_5.dll" />
    <ReferenceCopyLocalPaths Include="$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\Win32\Debug\cpprest120d_xp_2_5.pdb" />
  </ItemGroup>
  <ItemGroup Condition="'$(Configuration)' == 'Release' And '$(Platform)' == 'Win32' And '$(Force-Disable-cpprestsdk-v120-winxp-msvcstl-dyn-rt-dyn)' == ''">
    <ReferenceCopyLocalPaths Include="$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\Win32\Release\cpprest120_xp_2_5.dll" />
    <ReferenceCopyLocalPaths Include="$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\Win32\Release\cpprest120_xp_2_5.pdb" />
  </ItemGroup>
  <ItemGroup Condition="'$(Configuration)' == 'Debug' And '$(Platform)' == 'x64' And '$(Force-Disable-cpprestsdk-v120-winxp-msvcstl-dyn-rt-dyn)' == ''">
    <ReferenceCopyLocalPaths Include="$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\x64\Debug\cpprest120d_xp_2_5.dll" />
    <ReferenceCopyLocalPaths Include="$(MSBuildThisFileDirectory)..\..\lib\native\v120\winxp\msvcstl\dyn\rt-dyn\x64\Debug\cpprest120d_xp_2_5.pdb" />
